@model List<GroupWork.Models.UserModel>
<table class="table">
    <thead>
        <tr>
            <th>User Id</th>
            <th>User Name</th>
            <th>Is Active</th>
            <th>Employee ID</th>
            <th>Role Id</th>
            <th>Company Id</th>
            <th>Branch Id</th>
            <th>Added by</th>
            <th>Added Date</th>
            <th>Updated By</th>
            <th>Updated Date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@(user.IsActive == 1 ? "Active" : "Inactive")</td>
                <td>@(user.EmpId != null ? user.EmpId.ToString() : "N/A")</td>
                <td>@(user.RoleId.HasValue ? user.RoleId.Value.ToString() : "N/A")</td>
                <td>@(user.CompanyId.HasValue ? user.CompanyId.Value.ToString() : "N/A")</td>
                <td>@(user.BranchId.HasValue ? user.BranchId.Value.ToString() : "N/A")</td>
                <td>@user.AddedBy</td>
                <td>@(user.AddedDate != DateTime.MinValue ? user.AddedDate.ToString("yyyy-MM-dd HH:mm:ss") : "N/A")</td>
                <td>@user.UpdatedBy</td>
                <td>@(user.UpdatedDate != null ? user.UpdatedDate.Value.ToString("yyyy-MM-dd HH:mm:ss") : "N/A")</td>
                <td><a href="ViewUser/@user.Id" class="btn btn-dark">View</a></td>
            </tr>


        }
    </tbody>

</table>